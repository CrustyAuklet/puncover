{% extends "base.html.jinja" %}
{% block title %}Single File{% endblock %}
{% block content %}
    <h1>{{ file_name }}</h1>
    <h2>Functions</h2>
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>Code Size</th>
                <th>Stack Size</th>
            </tr>
        </thead>
        <tbody>
            {% for symbol in functions %}
            <tr>
                <td><a href="{{ symbol | symbol_url }}">{{ symbol.name }}</a></td>
                <td>{{ symbol.size }}</td>
                <td>{{ symbol.stack_size }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>

    {% if variables | length <= 0 %}
    <h2>No Static Variables</h2>
    {% else %}
    <h2>Static Variables</h2>
    <ul>
    {% for symbol in variables[:10] %}
        <li><a href="{{ symbol|symbol_file_url }}">{{ symbol.name|e }} {{ '(%d)' % symbol.size if symbol.size}}</a></li>
    {% endfor %}
    </ul>
    {% endif %}



{% endblock %}