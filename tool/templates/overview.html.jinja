{% extends "base.html.jinja" %}
{% block content %}

    <h2>Folders</h2>
    {% import 'lists.html.jinja' as lists with context %}
    {{ lists.directory(root_folders, []) }}

    <h2>Functions</h2>
    <ul>
    {% for symbol in all_functions[:100] %}
        <li><a href="{{ symbol|symbol_url }}">{{ symbol.name|e }} {{ '(%d)' % symbol.size if symbol.size}}</a></li>
    {% endfor %}
    </ul>

    <h2>Variables</h2>
    <ul>
    {% for symbol in all_variables[:100] %}
        <li><a href="{{ symbol|symbol_file_url }}">{{ symbol.name|e }} {{ '(%d)' % symbol.size if symbol.size}}</a></li>
    {% endfor %}
    </ul>

{% endblock %}
