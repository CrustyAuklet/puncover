{% extends "base.html.jinja" %}
{% block title %}Symbol{% endblock %}
{% block page_header %}
    <h1>{{ symbol.name }}</h1>
    <table class="table table-condensed">
        <tr>
            <th>file name</th>
            <td>
                {% if symbol.base_file %}
                <code><a href="{{ symbol | symbol_file_url }}">{{ symbol.base_file }}</a>:{{ symbol.line }}</code>
                {% else %}
                unknown file
                {% endif %}

            (0x{{ symbol.address }})
            </td>
        </tr>
        <tr>
            <th>function size</th>
            <td>{{ symbol.size }} bytes</td>
        </tr>
        <tr>
            <th>stack size</th>
        {% if symbol.stack_size %}
            <td>{{ symbol.stack_size }} bytes ({{ symbol.stack_qualifiers }})</td>
        {% else %}
            <td>unknown</td>
        {% endif %}
        </tr>
    </table>


{% endblock %}
{% block content %}
    <pre>
{% for line in symbol.asm %}{{ line | e | assembly }}
{% endfor %}
    </pre>
{% endblock %}